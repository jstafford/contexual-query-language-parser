{
  "name": "contexual-query-language-parser",
  "version": "1.0.0",
  "description": "Contextual Query Language (CQL) Parser in JavaScript",
  "keywords": [
    "CQL",
    "Contextual",
    "Common",
    "Query Language",
    "Parser"
  ],
  "bugs": "https://github.com/jstafford/contexual-query-language-parser/issues",
  "license": "BSD-3-Clause",
  "author": "Jakub Skoczen <jakub@indexdata.dk>",
  "contributors": [
    "Jason K Stafford <jason.stafford@scrycollective.com>"
  ],
  "main": "lib/cql.js",
  "directories": {
    "lib": "lib",
    "example": "example"
  },
  "repository": "https://github.com/jstafford/contexual-query-language-parser",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "test": "standard && mocha --require babel-register"
  },
  "dependencies": {
    "btoa": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-preset-env": "^1.3.3",
    "babel-register": "^6.24.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "standard": "^10.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "standard": {
    "ignore": [
      "/lib/"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
